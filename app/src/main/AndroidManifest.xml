<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quadsel.qvisionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QVisionApp"
        android:usesCleartextTraffic="true">
        <activity android:name="activities.LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activities.MenuScreen"/>
        <activity android:name="activities.EnquiryList"/>
        <activity android:name="activities.EnquiryView"/>
        <activity android:name="activities.NewEnquiryForm"/>
    </application>

</manifest>